<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jgsu.clong.mapper.UserMapper">

    <select id="queryUser" parameterType="jgsu.clong.bean.T_MALL_USER_ACCOUNT"
            resultType="jgsu.clong.bean.T_MALL_USER_ACCOUNT">
		select * from t_mall_user_account where yh_mch = #{yh_mch}
		and yh_mm = #{yh_mm}
	</select>
</mapper>